    "def insertion_sort(list):\n",
    "    for i in range(1, len(list)): \n",
    "        key = list[i]\n",
    "        \n",
    "        j = i-1 \n",
    "        \n",
    "        while j>=0 and key<list[j]: \n",
    "            list[j+1] = list[j]\n",
    "            j -= 1\n",
    "        list[j+1] = key"
   ]
  }
